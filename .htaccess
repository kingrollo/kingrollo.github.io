RewriteEngine on
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L] [NC,L]

RewriteRule ^(.*) . [NC,L]

<Files .pdf>
    ForceType application/octet-stream
    Header set Content-Disposition attachment
</Files>
